public class Examples {

	public static void main(String[] args) {
		int oldStyleInt;
		Integer integerObject;

		oldStyleInt = 42;
		System.out.println(oldStyleInt);

		// integerObject = new Integer(42); // ok, but not really needed
		integerObject = 42;
		System.out.println(integerObject);

		int foo = oldStyleInt + integerObject;
		System.out.println(foo);

		integerObject = 23 + 12 - foo;
		System.out.println(integerObject);
		
	}

}
